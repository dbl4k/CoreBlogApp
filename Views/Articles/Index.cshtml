@model List<Article>

<h2>@ViewBag.Title</h2>

<div>
    @if (Model != null && Model.Count > 0)
    {
        @foreach (Article article in Model)
        {
            <h2>@article.Title</h2>
            <p>@article.Content</p>
        }
    }
    else
    { 
        <p>no posts</p>
    }
    
</div>